{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "keeper",
    "version": "1.0.3"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": true,
        "execute": true,
        "open": true,
        "sidecar": true,
        "scope": [
          {
            "name": "flutter",
            "cmd": "flutter",
            "args": true
          },
          {
            "name": "git",
            "cmd": "git",
            "args": true
          },
          {
            "name": "ln",
            "cmd": "ln",
            "args": true
          },
          {
            "name": "mklink",
            "cmd": "mklink",
            "args": true
          },
          {
            "name": "open",
            "cmd": "open",
            "args": true
          }
        ]
      },
      "dialog": {
        "all": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": ["**"]
      },
      "os": {
        "all": true
      },
      "http": {
        "all": true,
        "request": true,
        "scope": ["http://**", "https://**"]
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.amos.keeper",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "windows": {
        "installMode": "passive"
      },
      "endpoints": [
        "https://gist.githubusercontent.com/WuJiuYou/729d73489785c31e0ae5450e7e58a44c/raw/keeper.releases.json",
        "https://gist.githubusercontent.com/WuJiuYou/729d73489785c31e0ae5450e7e58a44c/raw"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDUxNzk4OURDRTIyQkRDMzQKUldRMDNDdmkzSWw1VVo0clg2em41bS9Xa0pEN1ZMY3JrU01qRkRWZTU5Q09FWXJqT1VHYUxzcHMK"

    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "KEEPER",
        "width": 1000,
        "height": 800,
        "minWidth": 1000,
        "minHeight": 600
      }
    ]
  }
}
